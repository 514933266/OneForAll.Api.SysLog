{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  // 数据库连接字符串
  "ConnectionStrings": {
    "Default": "Data Source=.; Initial Catalog=OneForAll.SysLog; User Id=OneForAll; Password=OneForAll;Trust Server Certificate=true"
  },
  "Auth": {
    "ClientId": "OneForAll",
    "ClientSecret": "OneForAll",
    "ClientName": "系统日志服务",
    "ClientCode": "OneForAll.SysLog",
    "Authority": "http://localhost:5081"
  },
  "Cors": {
    "Origins": [ "*" ]
  },
  "HttpService": {
    "SysBase": "",
    "SysUms": "",
    "SysJob": "http://localhost:5086",
    // 群机器人webhook通知
    "WebhookUrl": ""
  },
  "Redis": {
    "IsEnabled": false,
    "InstanceName": "SysLog:",
    "ConnectionString": "127.0.0.1:6379"
  },
  "Quartz": {
    "IsEnabled": false,
    "AppId": "OneForAll.SysLog",
    "AppSecret": "OneForAll",
    "GroupName": "日志中心",
    "NodeName": "http://localhost:5084",
    "ScheduleJobs": [
      {
        "TypeName": "DeleteSysLogJob",
        "Cron": "0 0 2 * * ?",
        "Remark": "定时删除系统日志,每天凌晨2点执行一次"
      }
    ]
  }
}
